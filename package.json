{
  "name": "comic-reader",
  "version": "1.0.0",
  "description": "A modern manga/comic reader with real-time OCR text extraction and translation capabilities",
  "module": "src/server.ts",
  "type": "module",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deckyfx/manga-reader.git"
  },
  "author": "deckyfx",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/deckyfx/manga-reader/issues"
  },
  "homepage": "https://github.com/deckyfx/manga-reader#readme",
  "scripts": {
    "dev": "bun run src/server.ts",
    "typecheck": "tsc --noEmit",
    "build": "bun run build.ts",
    "migrate": "bun run src/db/migrate.ts",
    "db:generate": "drizzle-kit generate",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@elysiajs/eden": "^1.4.6",
    "@elysiajs/server-timing": "^1.4.0",
    "@elysiajs/static": "^1.4.7",
    "deepl-node": "^1.24.0",
    "drizzle-orm": "^0.45.1",
    "drizzle-typebox": "^0.3.3",
    "elysia": "^1.4.22",
    "fflate": "^0.8.2",
    "html2canvas": "^1.4.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-router-dom": "^7.13.0",
    "tailwindcss": "^4.1.18",
    "tesseract.js": "^7.0.0",
    "tsuki-logger": "^0.0.32"
  },
  "overrides": {
    "@sinclair/typebox": "0.34.0"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/bun": "latest",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "bun-plugin-tailwind": "^0.1.2",
    "drizzle-kit": "^0.31.8"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
